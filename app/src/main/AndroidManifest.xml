<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LabWeek2"
        tools:targetApi="31">
        <activity
            android:name=".Greeting"
            android:exported="false"
            android:label="@string/title_activity_greeting"
            android:theme="@style/Theme.LabWeek2" />
        <activity
            android:name=".Week3"
            android:exported="false"
            android:label="@string/title_activity_week3"
            android:theme="@style/Theme.LabWeek2" />
        <activity
            android:name=".QuizResult"
            android:exported="false"
            android:label="@string/title_activity_quiz_result"
            android:theme="@style/Theme.LabWeek2" />
        <activity
            android:name=".Quiz_Page"
            android:exported="false"
            android:label="@string/title_activity_quiz_page"
            android:theme="@style/Theme.LabWeek2" />
        <activity
            android:name=".Dashboard"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.LabWeek2" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.LabWeek2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>